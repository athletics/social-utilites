{
    "name": "athletics/social-utilites",
    "description": "Utilities for working with social networks.",
    "homepage": "https://github.com/athletics/social-utilites",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Athletics",
            "homepage": "http://athleticsnyc.com/"
        },
        {
            "name": "Matthew Spencer",
            "homepage": "https://matthewspencer.me/",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "wp-coding-standards/wpcs": "dev-master",
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "post-install-cmd": "vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs",
        "post-update-cmd": "vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs"
    }
}
